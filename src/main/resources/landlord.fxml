<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ta_ap.controllers.Landlord">
<!--    <TableView fx:id="tbv2_" prefWidth="1000" GridPane.columnIndex="0" GridPane.rowIndex="0">-->
    <TableView fx:id="tbv_" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <columns>
            <TableColumn fx:id="tbc_" prefWidth="500.0" text="Photo">
            </TableColumn>

            <TableColumn fx:id="tbcn_" prefWidth="323.0" text="Description">
            </TableColumn>
            <TableColumn fx:id="tbcp_" prefWidth="176.0" text="Phone Number" />
        </columns>

    </TableView>
<!--    </TableView>-->
    <TextField fx:id="tf_imageinfo" prefWidth="500.0" promptText="Enter URL" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button fx:id="bt_submit" onAction="#createObj" prefWidth="500.0" text="Add image" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <TextField fx:id="tf_description" promptText="Enter Description" GridPane.rowIndex="3" />
    <Button fx:id="bt_description" onAction="#add_description" prefWidth="500.0" text="Add Description" GridPane.rowIndex="4" />
    <TextField fx:id="tf_phone" promptText="Enter Phone Number" GridPane.rowIndex="5" />
    <Button fx:id="bt_phone" onAction="#btadd_phone" prefWidth="500.0" text="Add Phone Number" GridPane.rowIndex="6" />
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</GridPane>
